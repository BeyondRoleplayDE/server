        "@rollup/plugin-node-resolve": "^11.1.1",
        "@rollup/plugin-typescript": "^8.1.1",
        "@snowpack/plugin-typescript": "^1.2.1",
        "@types/gulp": "^4.0.8",
        "@types/gulp-debug": "^2.0.31",
        "@types/node": "^14.14.27",
        "altv-pkg": "^1.0.3",
        "cfg-reader": "^1.2.1",
        "fkill": "^7.1.0",
        "gulp": "^4.0.2",
